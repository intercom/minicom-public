{
  "name": "minicom-node",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/intercom/minicom-2.git"
  },
  "dependencies": {
    "express": "^4.22.1",
    "morgan": "^1.10.1",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "sequelize-cli": "^6.6.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=24.10.0"
  },
  "scripts": {
    "start": "tsx watch src/app.ts",
    "db:migrate": "sequelize-cli db:migrate",
    "db:rollback": "sequelize-cli db:migrate:undo",
    "db:create-migration": "sequelize-cli migration:generate --name",
    "typecheck": "tsc --noEmit"
  }
}
